<mvc:View controllerName="sap.ui.softwaris.flightapp.controller.Master" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"> 
	<List items="{flights>/Schedule}">
		<ObjectListItem title="{ path: 'flights>CARRID', formatter: '.formatter.airlineText' }" type="Navigation" press="onSelectFlight">
			<ObjectAttribute text="{flights>CITYFROM} to {flights>CITYTO}" />
			<ObjectAttribute text="Dep: {flights>DEPTIME} Arr: {flights>ARRTIME}" />
		</ObjectListItem>
	</List>
</mvc:View>