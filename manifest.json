{
  "_version": "1.12.0",
  "sap.app": {
	"id": "sap.ui.softwaris.flightapp",
	"type": "application",
	"i18n": "i18n/i18n.properties",
	"title": "{{appTitle}}",
	"description": "{{appDescription}}",
	"applicationVersion": {
	  "version": "1.0.0"
	}
  },
  "sap.ui": {
	"technology": "UI5",
	"deviceTypes": {
		"desktop": true,
		"tablet": true,
		"phone": true
	}
  },
  "sap.ui5": {
	"rootView": {
		"viewName": "sap.ui.softwaris.flightapp.view.App",
		"type": "XML",
		"async": true,
		"id": "app"
	},
	"dependencies": {
	  "minUI5Version": "1.60",
	  "libs": {
		"sap.m": {}
	  }
	},
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "sap.ui.softwaris.flightapp.i18n.i18n"
		}
	  },
	  "flights": {
		"type": "sap.ui.model.json.JSONModel",
		"uri": "Flights.json"	  	
	  }
	},
    "routing": {
	  "config": {
		"routerClass": "sap.m.routing.Router",
		"viewType": "XML",
		"viewPath": "sap.ui.softwaris.flightapp.view",
		"controlId": "app",
		"async": true
	  },
	  "routes": [
		{
		  "pattern": "",
		  "name": "master",
		  "target": "master"
		},	  	
		{
		  "pattern": "detail/{flightsPath}",
		  "name": "detail",
		  "target": "detail"
		},
		{
		  "pattern": "landing",
		  "name": "landing",
		  "target": "landing"
		}
	  ],
	  "targets": {
	  	"master": {
	  	  "viewId": "master",
	  	  "viewName": "Master",
	  	  "controlAggregation": "masterPages",
	  	  "viewLevel": 0
	  	},
		"detail": {
		  "viewId": "detail",
		  "viewName": "Detail",
		   "controlAggregation": "detailPages",
		   "viewLevel": 1
		},
		"landing": {
		  "viewId": "landing",
		  "viewName": "Landing",
		   "controlAggregation": "detailPages",
		   "viewLevel": 1
		}		
	  }
	}	
  }
}